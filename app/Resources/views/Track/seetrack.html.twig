    {# Header : titre + fil d'ariane #}
    {#-------------------------------------------------------------------------------------------------------------------------------------#}
    <section class="row page_cover">
        <div class="container">
            <div class="row m0">
                <h1>Détails de la musique</h1>
                <ol class="breadcrumb">
                    <li><a href="{{ path('home') }}">Accueil</a></li>
                    <li class="active">Détails de la musique</li>
                </ol>
            </div>
        </div>
    </section>


    {#<p>{{ track1.motsclefs  }}</p>#}
    <p></p>
    <div>Mots clefs :
        {% for motclef in track1.motsclefs %}
            <p>{{ motclef }}</p>
            <p>{{ track1.motsclefs[0] }}</p>
            {% if loop.first == false %} |
            {% endif %}
            {{ motclef.mot|capitalize }}
        {% else %}

            Ce track n'est lié à aucun mot clef
        {% endfor %}
    </div>

    {#<div>Groupes :#}
    {#{% for groupe in user.groupes %}#}
    {#{% if loop.first == false %} |  {% endif %}#}
    {#{{ groupe.name|capitalize }}#}
    {#{% else %}#}

    {#Cet utilisateur n'est lié à aucun groupe#}
    {#{% endfor %}#}
    {#</div>#}

    {% for motclef in motsclefs %}
        <p>{{ motclef.mot }}</p>
    {% endfor %}

    <form action="{{ path('seetrack', {'id' : track1.id}) }}" method="get">
        {% for motclef in motsclefs %}
            <label for="update-groupes-{{ motclef.mot }}">{{ motclef.mot }}</label>
            <input type="checkbox" {% if motclef in track1.motsclefs %}checked{% endif %} name="update-groupes[]"
                   id="update-groupes-{{ motclef.mot }}" value="{{ motclef.mot }}">
        {% endfor %}
        <input type="text" name="add-groupes">
        <input type="submit" value="Submit">
    </form>

    {#<form action="{{ path('users_update_groupe', {'id' : user.id}) }}" method="get">#}
    {#{% for groupe in groupes %}#}
    {#<label for="update-groupes-{{ groupe.name }}">{{ groupe.name }}</label>#}
    {#<input type="checkbox" {% if groupe in user.groupes %}checked{% endif %} name="update-groupes[]" id="update-groupes-{{groupe.name}}" value="{{ groupe.name }}">#}
    {#{% endfor %}#}
    {#<input type="text" name="add-groupes">#}
    {#<input type="submit" value="Submit">#}
    {#</form>#}





